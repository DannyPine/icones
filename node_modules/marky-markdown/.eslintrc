{
    "env": {
        "node": true,
        "es6": true
    },
    "rules": {
        "curly": [2, "multi-line"],
        "eqeqeq": 0,
        "generator-star-spacing": [2, "after"],
        "no-mixed-spaces-and-tabs": [2, "smart-tabs"],
        "no-trailing-spaces": 0,
        "no-underscore-dangle": 0,
        "no-unused-vars": [2, {"vars": "all", "args": "none"}],
        "no-use-before-define": 0,
        "semi": [2, "never"],
        "space-infix-ops": 0,
        "strict": 0
    },
    "globals": {
    }
}
