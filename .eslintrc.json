{
  "parser": "babel-eslint",
  "extends": [
    "standard",
    "prettier",
    "prettier/react",
    "prettier/standard",
    "plugin:react/recommended"
  ],
  "env": {
    "jest": true,
    "browser": true
  },
  "plugins": [
    "prettier",
    "react"
  ],
  "rules": {
    "prettier/prettier": "error",
    "no-unused-vars": "warn"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
